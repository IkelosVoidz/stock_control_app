<h2> Search Brands </h2>
<form action="/brands" method="GET">
    <label> Name </label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button tupe="submit"> Search </button>
</form>
<br>
<% brands.forEach(brand => {%>
    <div> 
        <%=brand.name  %>
        <a href="/brands/<%=brand.id%>/edit">Edit</a>
        <%- include('../partials/deleteForm' , {url: `/brands/${brand.id}`}) %>
    </div>
<% }) %>